<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewmodel="clr-namespace:BackOffice.Desk.UI.Common.ViewModel.ViewModels"
                    xmlns:views="clr-namespace:BackOffice.Desk.UI.Common.View">
    <DataTemplate DataType="{x:Type viewmodel:ClientsViewModel}">
        <views:ClientsView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:HomeViewModel}">
        <views:HomeView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:OrdersViewModel}">
        <views:OrdersView />
    </DataTemplate>
</ResourceDictionary>